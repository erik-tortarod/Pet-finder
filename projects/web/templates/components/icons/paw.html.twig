{# Paw Icon Component
   Parámetros disponibles:
   - size: tamaño del icono (default: 24)
   - class: clases CSS adicionales (default: '')
   - color: color del stroke (default: 'currentColor')
#}

{% set size = size|default(24) %}
{% set class = class|default('') %}
{% set color = color|default('currentColor') %}

<svg xmlns="http://www.w3.org/2000/svg"
     width="{{ size }}"
     height="{{ size }}"
     viewBox="0 0 24 24"
     fill="none"
     stroke="{{ color }}"
     stroke-width="2"
     stroke-linecap="round"
     stroke-linejoin="round"
     class="lucide lucide-paw-print {{ class }}">
    <circle cx="11" cy="4" r="2"/>
    <circle cx="18" cy="8" r="2"/>
    <circle cx="20" cy="16" r="2"/>
    <path d="M9 10a5 5 0 0 1 5 5v3.5a3.5 3.5 0 0 1-6.84 1.045Q6.52 17.48 4.46 16.84A3.5 3.5 0 0 1 5.5 10Z"/>
</svg>
